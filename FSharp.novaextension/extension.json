{
    "identifier": "com.spiltcocoa.FSharp",
    "name": "F#",
    "organization": "Spilt Cocoa",
    "description": "FSharp syntax and language server",
    "version": "1.0",
    "categories": ["languages"],
    
    "main": "main.js",
    
    "entitlements": {
        "process": true,
        "filesystem": "readonly"
